<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="705.0" prefWidth="1255.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pkgnew.addTaskMenuController">
    <children>
      <AnchorPane id="AnchorPane" layoutX="18.0" layoutY="-12.0" prefHeight="705.0" prefWidth="1255.0">
         <children>
            <TextField fx:id="nameTaskField" layoutX="559.0" layoutY="209.0" prefHeight="26.0" prefWidth="147.0" promptText="Task Name">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <effect>
                  <Blend />
               </effect>
            </TextField>
            <TextField fx:id="ownerField" layoutX="559.0" layoutY="242.0" prefHeight="26.0" prefWidth="147.0" promptText="Owner" />
            <TextField fx:id="categoryField" layoutX="559.0" layoutY="275.0" prefHeight="26.0" prefWidth="147.0" promptText="Category" />
            <DatePicker fx:id="datePicker" layoutX="559.0" layoutY="308.0" prefHeight="26.0" prefWidth="147.0" promptText="Due Date" />
            <ImageView fx:id="ratingOne" fitHeight="26.0" fitWidth="28.0" layoutX="558.0" layoutY="342.0" onMousePressed="#mousePressedStarOne" onMouseReleased="#mouseReleased" opacity="0.5" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@RatingStar.png" />
               </image>
            </ImageView>
            <ImageView fx:id="ratingTwo" fitHeight="26.0" fitWidth="28.0" layoutX="589.0" layoutY="342.0" onMousePressed="#mousePressedStarTwo" onMouseReleased="#mouseReleased" opacity="0.5" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@RatingStar.png" />
               </image>
            </ImageView>
            <ImageView fx:id="ratingThree" fitHeight="26.0" fitWidth="28.0" layoutX="619.5" layoutY="342.0" onMousePressed="#mousePressedStarThree" onMouseReleased="#mouseReleased" opacity="0.5" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@RatingStar.png" />
               </image>
            </ImageView>
            <ImageView fx:id="ratingFour" fitHeight="26.0" fitWidth="33.0" layoutX="650.0" layoutY="342.0" onMousePressed="#mousePressedStarFour" onMouseReleased="#mouseReleased" opacity="0.5" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@RatingStar.png" />
               </image>
            </ImageView>
            <ImageView fx:id="ratingFive" fitHeight="26.0" fitWidth="28.0" layoutX="681.0" layoutY="342.0" onMousePressed="#mousePressedStarFive" onMouseReleased="#mouseReleased" opacity="0.5" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@RatingStar.png" />
               </image>
            </ImageView>
            <Button fx:id="finalizeTaskButton" layoutX="594.0" layoutY="374.0" mnemonicParsing="false" onAction="#createTask" prefHeight="25.0" prefWidth="75.0" text="Done">
               <font>
                  <Font name="Verdana Bold" size="12.0" />
               </font></Button>
            <Button fx:id="back" layoutX="594.0" layoutY="405.0" mnemonicParsing="false" onAction="#viewMainMenu" prefHeight="25.0" prefWidth="75.0" text="Cancel">
               <font>
                  <Font name="Verdana Bold" size="12.0" />
               </font></Button>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
